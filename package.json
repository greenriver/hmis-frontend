{
  "name": "hmis-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.2.0"
  },
  "scripts": {
    "dev": "DEBUG=vite:proxy vite --clearScreen false",
    "check-types": "tsc --noEmit",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix --ext js,ts,jsx,tsx --ignore-path .gitignore .",
    "lint:check": "eslint --ext js,ts,jsx,tsx --ignore-path .gitignore .",
    "test": "NODE_ENV=test jest",
    "cert": "rm -rf .cert && mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'hmis.dev.test'",
    "dedup": "yarn-deduplicate -s fewer yarn.lock",
    "postinstall": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "graphql:codegen": "graphql-codegen --config codegen.yml && node scripts/generateEnumDescriptions.js && prettier --write ./src/types/* graphql.schema.json"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix --ignore-path .gitignore"
    ],
    "*": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
